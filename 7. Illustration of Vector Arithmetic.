
INPUT:

1.ADDITION:

a <- c(1, 2, 3)
b <- c(4, 5, 6)
c <- a + b
print(c)

OUTPUT:

[1] 5 7 9

2.Subtraction:

a <- c(4, 7, 2)
b <- c(1, 5, 6)
c <- a - b
print(c)

OUTPUT:

[1]  3  2 -4

3.MULTIPLICATION:

a <- c(4, 7, 2)
b <- c(1, 5, 6)
c <- a*b
print(c)

OUTPUT:

[1]  4 35 12

4.DIVISION:

a <- c(4, 7, 2)
b <- c(1, 5, 6)
c <- a/b
print(c)

OUTPUT:

[1] 4.0000000 1.4000000 0.3333333
